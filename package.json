{
  "name": "mcp-error-tracker",
  "version": "1.0.0",
  "description": "MCP server for tracking code errors, finding responsible developers, and creating JIRA tickets",
  "type": "module",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config build/index.js",
    "start:http": "node -r dotenv/config build/http-server.js",
    "start:compatible": "node -r dotenv/config build/compatible-server.js",
    "dev": "tsc --watch",
    "dev:http": "tsx --watch src/http-server.ts",
    "dev:compatible": "tsx --watch src/compatible-server.ts",
    "example:client": "tsx examples/client-example.ts",
    "test:compatible": "./test-compatible-server.sh"
  },
  "keywords": [
    "mcp",
    "error-tracking",
    "jira",
    "bitbucket"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
